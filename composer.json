{
    "name": "honlapbirodalom/laravel-chilkat",
    "description": "Laravel package for Chilkat PHP library.",
    "keywords": [
        "honlapbirodalom",
        "laravel",
        "laravel-chilkat",
        "laravel-package",
        "chilkat",
        "php",
        "composer",
        "composer-package"
    ],
    "homepage": "https://github.com/HonlapBirodalom/laravel-chilkat",
    "license": "MIT",
    "authors": [
        {
            "name": "Gábor Szentpéteri",
            "email": "webmaster@honlapbirodalom.hu",
            "role": "Solutions Architect"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "spatie/laravel-package-tools": "^1.16.0",
        "illuminate/contracts": "^9.28|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "nunomaduro/collision": "^7.10.0",
        "larastan/larastan": "^2.8.0",
        "orchestra/testbench": "^8.19.0",
        "pestphp/pest": "^2.30.0",
        "pestphp/pest-plugin-arch": "^2.6.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "spatie/laravel-ray": "^1.33.1"
    },
    "autoload": {
        "psr-4": {
            "HonlapBirodalom\\Chilkat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HonlapBirodalom\\Chilkat\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint --config pint.json",
        "bump-dev": "composer bump --dev-only"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HonlapBirodalom\\Chilkat\\ChilkatServiceProvider"
            ],
            "aliases": {
                "Chilkat": "HonlapBirodalom\\Chilkat\\Facades\\Chilkat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
